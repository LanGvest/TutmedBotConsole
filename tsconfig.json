{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES2015",
		"moduleResolution": "Node",
		"strict": true,
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"strictNullChecks": true,
		"strictBindCallApply": true,
		"declaration": false,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"sourceMap": false,
		"baseUrl": "./",
		"incremental": true,
		"esModuleInterop": true
	}
}